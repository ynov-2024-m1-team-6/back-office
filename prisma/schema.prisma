generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Product {
  id          Int     @id @default(autoincrement())
  username    String
  description String
  price       Decimal
  height      Float
  weight      Float
  ratio       String
  thumbnail   String

  // Ajoutez d'autres champs ou relations au besoin
}
